{
    /* CoD2 Windows client - command line arguments */
    "CoD2x.launch_args_win_client": [
        "+set net_ip localhost",
        "+set net_port 28961",
        "+set developer 2",
        "+set logfile 2",
        "+rcon login cod2x"
    ],

    /* CoD2 Windows client+server - command line arguments */
    "CoD2x.launch_args_win_client_server": [
        "+set net_ip localhost",
        "+set net_port 28960", 
        "+set developer 2",
        "+set sv_hostname \"CoD2x Developer Server+Client Windows\"",
        "+set showPacketStrings 1",
        "+set rcon_password cod2x",
        "+set logfile 2",
        "+set eyza leet",
        "+rcon login cod2x",
        "+devmap mp_toujane"
    ],

    /* CoD2 Windows server - command line arguments */
    "CoD2x.launch_args_win_server": [
        "+set net_ip localhost",
        "+set net_port 28960", 
        "+set dedicated 2",
        "+set developer 2",
        "+set sv_hostname CoD2x Developer Server Windows",
        "+set showPacketStrings 1",
        "+set logfile 2",
        "+set eyza leet",
        "+set rcon_password cod2x",
        "+devmap mp_toujane"
    ],

    /* CoD2 Linux server - command line arguments */
    "CoD2x.launch_args_linux": [
        "+set net_ip localhost",
        "+set net_port 28960",
        "+set dedicated 2",
        "+set developer 2",
        "+set sv_hostname CoD2x Developer Server Linux",
        "+set showPacketStrings 1",
        "+set logfile 2",
        "+set eyza leet",
        "+set rcon_password cod2x",
        "+devmap mp_toujane",
    ]
}